{
  "name": "Form Insights Chrome Extension",
  "description": "View the current form values with a click of a button",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "options_ui": {
    "page": "src/options/index.html",
    "open_in_tab": true
  },
  "chrome_url_overrides": {
    "newtab": "src/new-tab/index.html"
  },
  "permissions": ["storage"]
}
